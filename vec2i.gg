vec2i operator /(vec2i v1, i32 s) {
  vec2i retval; // = undefined
  retval.x = v1.x / s;
  retval.y = v1.y / s;
  return retval;
}

vec2i operator %(vec2i v1, i32 s) {
  vec2i retval; // = undefined
  retval.x = v1.x % s;
  retval.y = v1.y % s;
  return retval;
}

vec2i MulEl2(vec2i v1, vec2i v2) {
  vec2i retval; // = undefined
  retval.x = v1.x * v1.x;
  retval.y = v1.y * v1.y;
  return retval;
}

vec2i DivEl2(vec2i v1, vec2i v2) {
  vec2i retval; // = undefined
  retval.x = v1.x / v1.x;
  retval.y = v1.y / v1.y;
  return retval;
}

