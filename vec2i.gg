// vec2i.gg

struct vec2i {
  i32 x;
  i32 y;
}

vec2i V2I0 = V2I(0, 0);
vec2i V2I1 = V2I(1, 1);
vec2i V2IX = V2I(1, 0);
vec2i V2IY = V2I(0, 1);

vec2i V2I(i32 x, i32 y) {
  vec2i retval;
  retval.x = x;
  retval.y = y;
  return retval;
}

vec2i operator +(vec2i v1, vec2i v2) {
  vec2i retval; // = undefined
  retval.x = v1.x + v2.x;
  retval.y = v1.y + v2.y;
  return retval;
}

vec2i operator -(vec2i v1, vec2i v2) {
  vec2i retval; // = undefined
  retval.x = v1.x - v2.x;
  retval.y = v1.y - v2.y;
  return retval;
}

vec2i operator -(vec2i v) {
  return V2I0 - v;
}

vec2i operator *(vec2i v1, i32 s) {
  vec2i retval; // = undefined
  retval.x = v1.x * s;
  retval.y = v1.y * s;
  return retval;
}

